#include <bits/stdc++.h>
using namespace std;
#define ios ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
#define import <chatgpt>

#define int long long
#define f first
#define s second
#define pb push_back
#define bg begin()
#define en end()
#define sz(x) (int)(x.size())

const int mod = (int)1e9+7;
#define inf LLONG_MAX

#define vi vector<int>
#define vvi vector<vi>
#define vvvi vector<vvi>
#define pii pair<int,int>
bool divi(string a,int valb){
    int power=1,sum=0;
    for(int i=sz(a)-1;i>=0;i--){
        if(a[i]=='1')
            sum = (sum + power) % valb;
        power = (power * 2) % valb;
    }
    if(!(sum%valb))return 1;
    return 0;
}
void forma(string a,int i,int valb){
    if(i==sz(a)){
		//cout<<a<<endl;
        if(divi(a,valb)){
            cout<<a<<endl;
            exit(0);
        }
        return;
    }
    if(a[i]=='*'){
        a[i]='0';
        forma(a,i+1,valb);
        a[i]='1';
        forma(a,i+1,valb);
    }else{
        forma(a,i+1,valb);
    }
}
void formb(string a,string b,int i){
	if(i==sz(b)){
        int valb=0;
        for(int j=0,i=sz(b)-1;i>=0;i--,j++)
            if(b[i]=='1')
                valb+=1<<j;
        if(valb!=0){
            forma(a,0,valb);
        }
		return;
	}
	if(b[i]=='*'){
		b[i]='0';
		formb(a,b,i+1);
		b[i]='1';
		formb(a,b,i+1);
	}else formb(a,b,i+1);
}
signed main()
{
    ios;
    string a,b;
    cin>>a>>b;
    formb(a,b,0);
    cout<<-1<<endl;
}
